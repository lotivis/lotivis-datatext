{
  "name": "lotivis-datatext",
  "version": "1.0.0",
  "description": "GeoJson for lotivis.js",
  "author": "Lukas Danckwerth",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*.js",
    "src/**/*.js"
  ],
  "module": "src/index.js",
  "main": "src/index.js",
  "jsdelivr": "dist/lotivis-datatext.min.js",
  "unpkg": "dist/lotivis-datatext.min.js",
  "exports": {
    "umd": "./dist/lotivis-datatext.min.js",
    "default": "./src/index.js"
  },
  "scripts": {
    "build": "rollup --config",
    "build:watch": "rollup --config --watch",
    "test": "mocha test/**/*-test.js",
    "test:watch": "mocha test/**/*-test.js --watch",
    "serve": "http-server"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13",
    "http-server": "14",
    "mocha": "10",
    "node-sass": "7",
    "postcss": "8",
    "rollup": "2",
    "rollup-plugin-postcss": "4",
    "rollup-plugin-terser": "7"
  },
  "dependencies": {
    "lotivis-chart": "1",
    "lotivis-colors": "1",
    "lotivis-csv": "1",
    "lotivis-datasets": "1"
  }
}
